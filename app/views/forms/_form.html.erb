<%= form_with(model: form, local: true) do |form| %>


  <div class="form-group">
    <%= form.label :form_field_1 %>
    <%= form.text_field :form_field_1, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :form_field_2 %>
    <%= form.text_field :form_field_2, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :form_field_3 %>
    <%= form.text_field :form_field_3, class: "form-control" %>
  </div>

   <div class="form-group">
    <%= form.label :company_id %>
    <%= form.collection_select :company_id, Company.all, :id, :name, class: "form-control" %>          
  </div>

  <div class="form-group">
  <%= form.label :files %>
  <%= form.file_field :files, multiple: true, class: "form-control" %>
 </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
